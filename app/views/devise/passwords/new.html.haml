- @title = 'Reset password'

= form_for(resource, :as => resource_name, :url => password_path(resource_name)) do |f|
  = devise_error_messages!
  %fieldset
    %ol
      %li 
        = f.label :email
        = f.text_field :email
        
  = f.submit "Send me reset password instructions"

= render :partial => "devise/shared/links"